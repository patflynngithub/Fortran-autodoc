!> First FORD Fortran derived types sub module
submodule (types_mod) types_submod

contains

  module procedure set_value_my_class
     this%int_struct%value = value
  end procedure set_value_my_class
  
  module procedure set_value_my_class_extended
     call this%my_class_t%set_value(value)
     this%value_set = .TRUE.
  end procedure set_value_my_class_extended

  module procedure is_set
     is_set = this%value_set
  end procedure is_set

end submodule types_submod
